// "Fizz buzz has been used as an interview screening device for computer programmers.
// Writing a program to output the first 100 FizzBuzz numbers is a trivial problem for any would-be computer programmer,
// so interviewers can easily filter out those with insufficient programming ability."



using System;

namespace FizzBuzz
{
    class Program
    {
        static void Main(string[] args)
        {
           
            for (int i = 0; i < 100; i++)
            {
                Console.WriteLine(100 - i);
            }

            Console.WriteLine();
            Console.WriteLine();

            for (int i = 1; i <= 100; i++)
            {

                if (i % 3 == 0 && i % 5 == 0)
                {
                    Console.WriteLine("FizzBuzz");
                }
                else if (i % 3 == 0)
                {
                    Console.WriteLine("Fizz");
                }
                else if (i % 5 == 0)
                {
                    Console.WriteLine("Buzz");
                }
                else
                {
                    Console.WriteLine(i);
                }
            }
        }
    }
}
